on early-init
    insmod /rk29xxnand_ko.ko
    start ueventd

on init
    export PATH /system/bin:/sbin
    export ANDROID_ROOT /system
    export ANDROID_DATA /data
    export EXTERNAL_STORAGE /sdcard

    symlink /system/etc /etc

    mkdir /system
    mkdir /data
    mkdir /cache
    mkdir /emmc
    mkdir /sdcard
    mkdir /sd-ext
    mount tmpfs tmpfs /tmp
#    mount vfat /dev/block/mtdblock10 /emmc
    
    write /sys/devices/virtual/bdi/179:0/read_ahead_kb 8192
    write /sys/devices/virtual/bdi/7:0/read_ahead_kb 8192
    write /sys/devices/virtual/bdi/93:0/read_ahead_kb 8192

    write /sys/class/block/mmcblk0/bdi/read_ahead_kb 8192
    write /sys/class/block/mtdblock0/bdi/read_ahead_kb 8192
    write /sys/class/block/mtdblock1/bdi/read_ahead_kb 8192
    write /sys/class/block/mtdblock2/bdi/read_ahead_kb 8192
    write /sys/class/block/mtdblock3/bdi/read_ahead_kb 8192
    write /sys/class/block/mtdblock4/bdi/read_ahead_kb 8192
    write /sys/class/block/mtdblock5/bdi/read_ahead_kb 8192
    write /sys/class/block/mtdblock6/bdi/read_ahead_kb 8192
    write /sys/class/block/mtdblock7/bdi/read_ahead_kb 8192
    write /sys/class/block/mtdblock8/bdi/read_ahead_kb 8192
    write /sys/class/block/mtdblock9/bdi/read_ahead_kb 8192
    write /sys/class/block/mtdblock10/bdi/read_ahead_kb 8192

    write /proc/sys/kernel/hung_task_timeout_secs 0    

on boot
    setprop service.adb.root 1
    ifup lo
    hostname localhost
    domainname localdomain

    class_start default

service ueventd /sbin/ueventd
    critical

service erase_misc /sbin/sh -c "busybox dd if=/misc.img of=/dev/block/mtdblock1; sync"
    oneshot

service recovery /sbin/recovery

service adbd /sbin/adbd recovery
    disabled

# Always start adbd on userdebug and eng builds
on property:ro.debuggable=1
    write /sys/class/android_usb/android0/enable 0
    write /sys/class/android_usb/android0/idVendor 18D1
    write /sys/class/android_usb/android0/idProduct D001
    write /sys/class/android_usb/android0/functions mass_storage,adb
    write /sys/class/android_usb/android0/enable 1
    write /sys/class/android_usb/android0/iManufacturer $ro.product.manufacturer
    write /sys/class/android_usb/android0/iProduct $ro.product.model
    write /sys/class/android_usb/android0/iSerial $ro.serialno
    start adbd

# Restart adbd so it can run as root
on property:service.adb.root=1
    write /sys/class/android_usb/android0/enable 0
    restart adbd
    write /sys/class/android_usb/android0/enable 1
